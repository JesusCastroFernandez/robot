# Importamos la biblioteca necesaria para controlar el robot
import robot

# Inicializamos el robot
robot.inicializar()

# Movemos el robot hacia adelante
robot.avanzar()

# Gira el robot a la derecha
robot.girar_derecha()

# Movemos el robot hacia atrás
robot.retroceder()

# Giramos el robot a la izquierda
robot.girar_izquierda()

# Detenemos el robot
robot.detener()

# Cerramos la conexión con el robot
robot.cerrar()
